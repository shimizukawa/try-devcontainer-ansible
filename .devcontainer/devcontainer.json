{
	"name": "Ubuntu",
	"build": {
		"dockerfile": "Dockerfile",
	},

	// "forwardPorts": [],

	"remoteUser": "vscode",
	"features": {
		"docker-in-docker": "latest",
		"git": "latest",
		"github-cli": "latest",
		"python": "os-provided"
	},

	"containerEnv": {
		"LC_ALL": "en_US.UTF-8"
	},

	"postCreateCommand": "bash .devcontainer/post_create_command.sh",
	"postStartCommand": "bash .devcontainer/post_start_command.sh"
}
